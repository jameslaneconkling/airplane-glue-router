{
  "name": "juno-falcor-router",
  "version": "0.1.0",
  "description": "falcor router for juno knowledge graphs",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint src/",
    "test": "NODE_ENV=test ts-node test/ | tap-summary",
    "validate": "npm run lint && npm run test",
    "clean": "rm -r ./dist",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "npm run clean && tsc",
    "build:tests": "tsc --project tsconfig-test.json",
    "start": "NODE_ENV=production node app/index.js",
    "dropDB": "rm -r ./level",
    "seedDB": "node ./lib/seed_db.js",
    "dumpDB": "node ./lib/dump_db.js",
    "seedDBAndStart": "npm run seedDB && npm run start"
  },
  "author": "jameslaneconkling",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.16.3",
    "falcor-express": "^0.1.4",
    "falcor-router": "^0.8.3",
    "io-ts": "^1.3.0",
    "leveldown": "^1.6.0",
    "levelgraph": "^2.1.1",
    "levelgraph-n3": "^2.1.0",
    "levelup": "^1.3.3",
    "memdown": "^1.2.4",
    "morgan": "^1.9.1",
    "query-string": "^6.2.0",
    "ramda": "^0.21.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/falcor-express": "^0.1.18",
    "@types/morgan": "^1.7.35",
    "@types/query-string": "^6.1.0",
    "@types/ramda": "github:types/npm-ramda#dist",
    "@types/tape": "^4.2.32",
    "falcor": "^1.1.0",
    "falcor-supertest-datasource": "^1.1.0",
    "nodemon": "^1.18.3",
    "supertest": "^2.0.0",
    "tap-summary": "^3.0.2",
    "tape": "^4.9.1",
    "ts-node": "^6.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  }
}
